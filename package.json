{
  "name": "declarapi-example",
  "version": "1.0.0",
  "description": "An example project using declarapi",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "dev": "tsc-watch --onSuccess 'node ./dist/index.js'",
    "prestart": "tslint -c tslint.json -p tsconfig.json --fix",
    "start": "tsc && node ./dist/index.js",
    "test": "jest",
    "generate": "declarapi"
  },
  "eslintConfig": {
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    }
  },
  "standardx": {
    "env": [
      "jest"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "preset": "ts-jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmagyar/declarapi-example.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mmagyar/declarapi-example/issues"
  },
  "homepage": "https://github.com/mmagyar/declarapi-example#readme",
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "jest": "^26.0.1",
    "node-fetch": "^2.6.0",
    "standardx": "^5.0.0",
    "ts-jest": "^26.1.1",
    "tsc-watch": "^4.2.9",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "declarapi": "^0.5.0",
    "express": "^4.17.1"
  }
}
